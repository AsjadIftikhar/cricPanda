
@{
    Layout = null;
}

@using db_connectivity.Models;
@model List<User>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>homePage</title>
</head>
<body>
    <div>

        <table border="1">
            <thead><tr><td>User ID</td> <td>Password</td> <td>Date of Birth</td></tr></thead>

            @foreach (User user in Model)
            {
                <tr>
                    <td>@user.userId</td>
                    <td>@user.password</td>
                    <td>@user.dateOfBirth</td>
                </tr>
            }

        </table>

    </div>
</body>
</html>
